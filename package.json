{
  "name": "crazy-parser",
  "version": "1.4.0",
  "description": "A light-weight parser combinator",
  "type": "module",
  "files": [
    "./src/**/*.ts",
    "./src/**/*.js",
    "./src/**/*.d.ts"
  ],
  "exports": {
    ".": "./src/index.js",
    "./*": "./src/*.js",
    "./json/*": "./src/json/*.js"
  },
  "scripts": {
    "build": "cd src && tsc",
    "test": "vitest run --coverage",
    "clean": "rm -f ./crazy-parser-*.tgz; rm -f ./src/*.js; rm -f ./src/*.d.ts"
  },
  "keywords": ["parser", "combinator", "typescript"],
  "author": "chen-zhanming",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/SpeedyOrc-C/crazy-parser"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
